<form method="post" action="{{ path('websites_delete', {'idWeb': website.idWeb}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ website.idWeb) }}">
    <button class="btn">Delete</button>
</form>
